{
  "name": "tezblock",
  "version": "1.0.0",
  "private": true,
  "description": "tezblock",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "yarn set-env && ng build && yarn reset-env",
    "test": "ng test",
    "test-ci": "yarn set-env && node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng test --code-coverage --watch=false && yarn reset-env",
    "lint": "ng lint --format stylish",
    "lint-ci": "ng lint --format json > ./lintReport.json || true",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "prettier": "prettier --write 'src/**/*.ts' 'src/**/*.html' 'config/**/*' 'README.md'",
    "build:prod": "yarn set-env && ng build --prod && yarn reset-env",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "postinstall": "yarn husky install && node config/patch_crypto.js || true",
    "prestart": "yarn set-env",
    "poststart": "yarn reset-env",
    "pretest": "yarn set-env",
    "posttest": "yarn reset-env",
    "prepare-prod-build": "yarn disable-pure-getters && yarn configure-mangle",
    "disable-pure-getters": "replace 'pure_getters: buildOptions.buildOptimizer' 'pure_getters: false' ./node_modules/@angular-devkit/build-angular/src/angular-cli-files/models/webpack-configs/common.js ",
    "configure-mangle": "replace 'safari10: true' 'safari10: true, keep_fnames: true' ./node_modules/@angular-devkit/build-angular/src/angular-cli-files/models/webpack-configs/common.js ",
    "sonar-scanner": "sonar-scanner",
    "render": "ts-node prerender.ts",
    "set-env": "bash ./scripts/add-environment-variables.sh",
    "reset-env": "bash ./scripts/remove-environment-variables.sh"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
