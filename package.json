{
  "name": "tezblock",
  "version": "1.0.0",
  "private": true,
  "description": "tezblock",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run set-env && ng build && npm run reset-env",
    "test": "ng test",
    "test-ci": "npm run set-env && ng test --code-coverage --watch=false && npm run reset-env",
    "lint": "ng lint --format stylish",
    "lint-ci": "ng lint --format json > ./lintReport.json || true",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "prettier": "prettier --write \"src/**/*.ts\" \"e2e/**/*.ts\" \"src/**/*.js\" \"test/**/*.js\"",
    "build:prod": "npm run set-env && ng build --prod && npm run reset-env",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "postinstall": "node config/patch_crypto.js || true",
    "prestart": "npm run set-env",
    "poststart": "npm run reset-env",
    "pretest": "npm run set-env",
    "posttest": "npm run reset-env",
    "prepare-prod-build": "npm run disable-pure-getters && npm run configure-mangle",
    "disable-pure-getters": "replace 'pure_getters: buildOptions.buildOptimizer' 'pure_getters: false' ./node_modules/@angular-devkit/build-angular/src/angular-cli-files/models/webpack-configs/common.js ",
    "configure-mangle": "replace 'safari10: true' 'safari10: true, keep_fnames: true' ./node_modules/@angular-devkit/build-angular/src/angular-cli-files/models/webpack-configs/common.js ",
    "sonar-scanner": "sonar-scanner",
    "render": "ts-node prerender.ts",
    "set-env": "bash ./scripts/add-environment-variables.sh",
    "reset-env": "bash ./scripts/remove-environment-variables.sh"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
